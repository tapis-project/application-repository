{
  "id": "<username>-basic-shell-script-fork-docker",
  "version": "0.1",
  "description": "A basic shell script application run using Docker in FORK mode.",
  "jobType": "FORK",
  "runtime": "DOCKER",
  "containerImage": "tapis/basic-shell-script-app:0.1",
  "jobAttributes": {
    "parameterSet": {
      "appArgs": [
      {
        "arg": "arg1",
        "name": "appArg1Name",
        "description": "description for app arg 1",
        "inputMode": "FIXED",
        "notes": { "arg1note": "my first arg" }
      },
      {
        "arg": "arg2",
        "name": "appArg2Name",
        "description": "description for app arg 2",
        "inputMode": "FIXED",
        "notes": { "arg2note": "my 2nd arg" }
      }
      ],
      "envVariables": [
      {
        "key": "MY_ENV1",
        "value": "env_1_value",
        "description": "my env var #1"
      }
      ]
    }
  }
}
